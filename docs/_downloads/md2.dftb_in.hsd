Geometry = GenFormat {
  <<< "md2.geom.gen"
}

Driver = SecondDerivatives {
  Delta = 1e-4
}


Hamiltonian = DFTB {
  SCC = Yes
  SCCTolerance = 1E-8
  Filling = Fermi {
    Temperature [Kelvin] = 400
  }
  SlaterKosterFiles = Type2FileNames {
    Prefix = "../../slako/"
    Separator = "-"
    Suffix = ".skf" 
  }
  MaxAngularMomentum = {
    C = "p"
    O = "p"
    H = "s"
  }
}
